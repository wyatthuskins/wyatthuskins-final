{% extends '_layouts/base.njk' %}

{% block page_title %}Discography{% endblock %}

{% block page_description %}The albumns of Queen{% endblock %}

{% block content %}
<article class="container">
  <div class="row">
    <div class="col-sm-12 text-center">
      {% block blurb %}
        <h1 class="disc">Discography</h1>
      {% endblock %}
    </div>
  </div>
  <div class="card-columns">
    <div class="card">
      <img class="card-img-top" src="img/img/discs/Queen_Queen.png" alt="Card image cap">
      <div class="card-body">
        <h5 class="card-title">Queen</h5>
        <p class="card-text">Release Date: 1973</p>
      </div>
    </div>
    <div class="card">
      <img class="card-img-top" src="img/img/discs/Queen_Queen_II.jpg" alt="Card image cap">
